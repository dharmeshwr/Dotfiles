super + Return
  ghostty

super + @space
  dmenu_run

super + f
  zen

super + d
  zen -P work 

alt + 0
  nautilus

ctrl + shift + e
  rofi -show emoji -theme ~/.config/rofi/theme.rasi

ctrl + shift + p
  zen --private-window

# make sxhkd reload its configuration files:
super + Escape
	pkill -USR1 -x sxhkd

