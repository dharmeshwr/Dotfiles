super + f
  zen

alt + 0
  pcmanfm

alt + 9
  slock

ctrl + shift + p
  firefox --private-window

super + p
  pavucontrol

super + Escape
	pkill -USR1 -x sxhkd

XF86MonBrightness{Up,Down}  
  brightnessctl set 10{+,-}% 

XF86AudioRaiseVolume
  wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+

XF86AudioLowerVolume
  wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-

XF86AudioMute
  wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle

super + shift + p
  sudo poweroff

super + shift + r
  sudo reboot now

Print  
  flameshot screen --path ~/Pictures/Screenshots/screenshot

shift + Print 
  flameshot gui --path ~/Pictures/Screenshots/screenshot

ctrl + Print  
  flameshot gui --clipboard
